{
  "name": "vue-simple-icons",
  "version": "4.7.0",
  "homepage": "https://vue-simple-icons.mainvest.com",
  "description": "Simple Icons as Vue components.",
  "repository": {
    "url": "mainvest/vue-simple-icons",
    "type": "git"
  },
  "main": "dist/vue-simple-icons.cjs.js",
  "cdn": "dist/vue-simple-icons.min.js",
  "unpkg": "dist/vue-simple-icons.min.js",
  "module": "dist/vue-simple-icons.es.js",
  "files": [
    "dist",
    "icons"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "echo 'no tests!'",
    "prepublishOnly": "npm run build",
    "build": "npm run build:icons && npm run build:dist",
    "build:dist": "bili --format umd,es,cjs,umd-min --js buble --name vue-simple-icons",
    "build:icons": "node build"
  },
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "bili": "^5.0.5",
    "eslint-config-rem": "^4.0.0",
    "fs-extra": "^10.1.0",
    "lodash.kebabcase": "^4.1.1",
    "marked": "^4.0.17",
    "pascal-case": "^3.1.2",
    "prismjs": "^1.28.0",
    "simple-icons": "^7.4.0",
    "typeface-nunito": "^1.1.13",
    "v-tippy": "^2.0.0",
    "vue": "^3.2.37",
    "vue-github-badge": "^1.0.1",
    "written-number": "^0.11.1"
